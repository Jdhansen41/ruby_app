<h1>All Tags</h1>
<ul>
    <% @tags.each do |tag| %>
    <li>
        <%= link_to tag.name, tag_path(tag) %>
        <h4><%= link_to "delete", tag_path(tag), method: :delete, data: {confirm: "Really delete the tag?"} %></h4>
    </li>
    <% end %>
</ul>
<p><%= link_to "Back to Articles", articles_path%></p>